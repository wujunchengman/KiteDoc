<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <SignAssembly>false</SignAssembly>
    <Version>0.0.8</Version>
    <Company>吴俊城</Company>
    <Product>吴俊城</Product>
    <Description>一组使用OpenXML操作Word文档的快捷API</Description>
    <PackageProjectUrl>weekok.cn</PackageProjectUrl>
    <Authors>吴俊城</Authors>
    <NeutralLanguage>zh</NeutralLanguage>
    <RepositoryUrl>https://github.com/wujunchengman/KiteDoc</RepositoryUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageReleaseNotes>添加根据文本替换文本的API replaceText</PackageReleaseNotes>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="*" />
    <PackageReference Include="SixLabors.ImageSharp" Version="*" />
    <PackageReference Include="Syncfusion.DocIORenderer.Net.Core" Version="*" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
